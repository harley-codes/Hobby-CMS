{
	"name": "hobby-cms",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"dev:t": "next dev --turbo",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"lint:fix": "next lint --fix",
		"build:prisma-cockroach": "bun run db:prisma-cockroach:generate && bun run build",
		"build:prisma-postgres": "bun run db:prisma-postgres:generate && bun run build",
		"db:prisma-cockroach:generate": "prisma generate --schema=src/modules/database/vendors/prisma-cockroach/schema.prisma",
		"db:prisma-cockroach:migrate": "prisma migrate dev --name migrate --schema=src/modules/database/vendors/prisma-cockroach/schema.prisma",
		"db:prisma-cockroach:push": "prisma db push --schema=src/modules/database/vendors/prisma-cockroach/schema.prisma",
		"db:prisma-postgres:generate": "prisma generate --schema=src/modules/database/vendors/prisma-postgres/schema.prisma",
		"db:prisma-postgres:migrate": "prisma migrate dev --name migrate --schema=src/modules/database/vendors/prisma-postgres/schema.prisma",
		"db:prisma-postgres:push": "prisma db push --schema=src/modules/database/vendors/prisma-postgres/schema.prisma"
	},
	"dependencies": {
		"@emotion/cache": "^11.14.0",
		"@emotion/react": "^11.14.0",
		"@emotion/styled": "^11.14.0",
		"@fontsource/roboto": "^5.2.5",
		"@mui/icons-material": "^7.1.0",
		"@mui/material": "^7.1.0",
		"@mui/material-nextjs": "^7.1.1",
		"@mui/x-charts": "^8.5.0",
		"@mui/x-date-pickers": "^8.5.0",
		"@prisma/client": "6.8.2",
		"@types/luxon": "^3.6.2",
		"@types/uuid": "^10.0.0",
		"jodit-react": "^5.2.19",
		"linq": "^4.0.3",
		"luxon": "^3.6.1",
		"next": "15.3.3",
		"next-auth": "^4.24.11",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-icons": "^5.5.0",
		"sass": "^1.89.1",
		"uuid": "^11.1.0"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3",
		"@types/node": "^20",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"eslint": "^9",
		"eslint-config-next": "15.3.3",
		"jiti": "^2.4.2",
		"prisma": "6.8.2",
		"typescript": "^5"
	}
}
